#include<stdio.h>
#include<conio.h>
int add(int a,int b)
{
    return a+b;
}
int subtract(int a,int b)
{
    return a-b;
}
int multiply(int a,int b)
{
    return a*b;
}
float divide(int a,int b)
{
    return (float)a/b;
}
main()
{
    int a,b;
    scanf("%d %d",&a,&b);
    printf("%d\n",add(a,b));
    printf("%d\n",subtract(a,b));
    printf("%d\n",multiply(a,b));
    printf("%.2f\n",divide(a,b));
}
