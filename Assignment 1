#include<stdio.h>
#include<conio.h>
void big(int a,int b,int c)
{
    int biggest;
    if (a>b&&a>c)
    {
        biggest=a;
    }
    else if (b>a&&b>c)
    {
        biggest=b;
    }
    else
    {
        biggest=c;
    }
    printf("%d\n",biggest);
}
main()
{
    int num1,num2,num3;
    scanf("%d",&num1);
    scanf("%d",&num2);
    scanf("%d",&num3);
    big(num1,num2,num3);
}
